{
    "name": "kode/context",
    "description": "A lightweight, high-performance context management library for PHP coroutine/fiber environments",
    "keywords": ["context", "coroutine", "fiber", "swoole", "swow", "request-context"],
    "homepage": "https://github.com/kodephp/context",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Kode Team",
            "email": "team@kodephp.com"
        }
    ],
    "support": {
        "issues": "https://github.com/kodephp/context/issues",
        "source": "https://github.com/kodephp/context"
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Kode\\Context\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kode\\Context\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "analyse": "phpstan analyse src tests --level=max"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    },
    "version": "1.1.0",
    "config": {
        "sort-packages": true
    }
}